<h1>Listing log_hours</h1>

<table>
  <thead>
    <tr>
      <th>Hours</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @log_hours.each do |log_hour| %>
      <tr>
        <td><%= log_hour.hours %></td>
        <td><%= log_hour.user %></td>
        <td><%= link_to 'Show', log_hour %></td>
        <td><%= link_to 'Edit', edit_log_hour_path(log_hour) %></td>
        <td><%= link_to 'Destroy', log_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log hour', new_log_hour_path %>
